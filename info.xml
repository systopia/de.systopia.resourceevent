<?xml version="1.0"?>
<extension key="de.systopia.resourceevent" type="module">
  <file>resourceevent</file>
  <name>CiviResource Event</name>
  <description>Resource Management for CiviCRM Events</description>
  <license>AGPL-3.0</license>
  <maintainer>
    <author>SYSTOPIA</author>
    <email>info@systopia.de</email>
  </maintainer>
  <urls>
    <url desc="Main Extension Page">https://github.com/systopia/de.systopia.resourceevent</url>
    <url desc="Documentation">https://github.com/systopia/de.systopia.resourceevent</url>
    <url desc="Support">https://github.com/systopia/de.systopia.resourceevent/issues</url>
    <url desc="Licensing">http://www.gnu.org/licenses/agpl-3.0.html</url>
  </urls>
  <releaseDate>2023-07-04</releaseDate>
  <version>1.0-beta2</version>
  <develStage>beta</develStage>
  <compatibility>
    <ver>5.27</ver>
  </compatibility>
  <comments/>
  <requires>
    <ext>de.systopia.resource</ext>
  </requires>
  <classloader>
    <psr4 prefix="Civi\" path="Civi"/>
  </classloader>
  <civix>
    <namespace>CRM/Resourceevent</namespace>
    <angularModule>crmResourceevent</angularModule>
    <format>23.02.1</format>
  </civix>
  <mixins>
    <mixin>menu-xml@1.0.0</mixin>
    <mixin>setting-php@1.0.0</mixin>
  </mixins>
</extension>
